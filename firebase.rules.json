{
  "rules": {
    ".read": "false",
    ".write": "false",
    "scores": {
      ".read": "true",
      ".write": "true",
      ".indexOn": "score",
      "$uid": {
        ".validate": "newData.child('name').isString() && newData.child('name').val().length == 3 && newData.child('score').isNumber() && !data.exists()"
      }
    }
  }
}
